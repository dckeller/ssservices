<%= bootstrap_form_for @slot do |f| %>

	<%= f.date_field :date %>

	<%= f.text_field :city %>

	<%= f.hidden_field :state, value: "Utah" %>

	<%= f.select :contact, options_for_select(contact_options) %>

	<%= f.time_field :start_time %>

	<%= f.time_field :end_time %><br>

	<%= f.submit "Add Slot" %><br>
<% end %>